<% if (products.contents.length != 0) { %>
<div class="" style="text-align:center;">
    <ul class="pagination">
        <li <%if(products.page == 1){%> class="disabled" <%}else{%>class="waves-effect" <%}%> >
            <a <%if(products.page > 1){%> href="?page=<%=products.page-1%>" <%}%>>
                <i class="material-icons">chevron_left</i>
            </a>
        </li>
        <% for(var i=products.pnStart; i<=products.pnEnd; i++){ %> <li <%if(i===products.page){%> class="active"
                <%}%>><a href="?page=<%=i%>"><%=i%></a></li>
        <% } %>
        <li <%if(products.page == products.pnTotal){%> class="disabled" <%}else{%> class="waves-effect" <%}%>>
            <a <%if(products.page < products.pnTotal){%> href="?page=<%=products.page+1%>" <%}%> class="waves-effect">
                <i class="material-icons">chevron_right</i>
            </a>
        </li>
    </ul>
</div>
<% } %>